    "url": "https://github.com/kcansari/proshop/issues"
  },
  "homepage": "https://github.com/kcansari/proshop#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.3",
    "multer": "^1.4.5-lts.1",
    "react-paypal-button-v2": "^2.6.3"
