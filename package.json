{
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN shopping cart",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
